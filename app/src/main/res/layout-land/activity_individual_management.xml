<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".IndividualManagementActivity">

    <ImageButton
        android:id="@+id/healthCheck"
        android:layout_width="283dp"
        android:layout_height="269dp"
        android:background="#00000000"
        android:src="@drawable/tab_icon_kyuji"
        tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="743dp"
        tools:layout_editor_absoluteY="291dp" />

    <TextView
        android:layout_width="258dp"
        android:layout_height="67dp"
        android:gravity="center_horizontal"
        android:text="健康チェック"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/feedRecord"
        app:layout_constraintTop_toTopOf="@+id/feedRecord"
        app:layout_constraintVertical_bias="0.608"
        tools:ignore="HardcodedText" />

    <TextView
        android:layout_width="287dp"
        android:layout_height="60dp"
        android:gravity="center_horizontal"
        android:text="給餌記録"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/healthCheck"
        app:layout_constraintTop_toTopOf="@+id/healthCheck"
        app:layout_constraintVertical_bias="0.599"
        tools:ignore="HardcodedText" />

    <ImageButton
        android:id="@+id/feedRecord"
        android:layout_width="265dp"
        android:layout_height="258dp"
        android:background="#00000000"
        android:src="@drawable/tab_icon_health_check"
        tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="302dp"
        tools:layout_editor_absoluteY="291dp" />

    <ImageButton
        android:id="@+id/contact"
        android:layout_width="130dp"
        android:layout_height="111dp"
        android:background="#00000000"
        android:padding="0dp"
        android:src="@drawable/tab_phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton2"
        tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/imageButton2"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:background="#4ea1d3"
        android:text=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="個体管理"
        android:textColor="#FFFFFF"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,MissingConstraints" />

    <ImageButton
        android:id="@+id/back"
        android:layout_width="93dp"
        android:layout_height="50dp"
        android:background="#00000000"
        android:padding="0dp"
        android:src="@drawable/tab_back"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="ContentDescription,TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:background="#4ea1d3"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/btnVoice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="222dp"
        android:background="#00000000"
        android:elevation="3dp"
        android:padding="0dp"
        android:src="@drawable/tab_navi_button2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/btnMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="222dp"
        android:background="#00000000"
        android:elevation="3dp"
        android:padding="0dp"
        android:src="@drawable/tab_navi_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>