<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F8F7F5"
    android:fitsSystemWindows="true"
    tools:context=".MainActivity2">


    <ScrollView
        android:layout_width="886dp"
        android:layout_height="405dp"
        android:layout_marginTop="92dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contact"
        tools:ignore="RedundantNamespace">

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="300dp"
            android:background="#F8F7F5"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.49"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/contact"
            tools:ignore="RedundantNamespace">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/btn_nippo"
                    android:layout_width="443dp"
                    android:layout_height="wrap_content"
                    android:background="#F8F7F5"
                    android:src="@drawable/tab_icon_nippo"
                    tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck"
                    tools:layout_editor_absoluteX="302dp"
                    tools:layout_editor_absoluteY="291dp" />

                <ImageButton
                    android:layout_width="443dp"
                    android:layout_height="wrap_content"
                    android:background="#F8F7F5"
                    android:src="@drawable/tab_icon_kotaikanri"
                    tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck"
                    tools:layout_editor_absoluteX="302dp"
                    tools:layout_editor_absoluteY="291dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="114dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="443dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:paddingTop="30dp"
                    android:text="飼育個体管理"
                    android:textSize="30sp"
                    tools:ignore="HardcodedText" />

                <TextView
                    android:layout_width="443dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:paddingTop="30dp"
                    android:text="給餌記録認証"
                    android:textSize="30sp"
                    tools:ignore="HardcodedText" />

            </LinearLayout>


        </LinearLayout>
    </ScrollView>

    <ImageButton
        android:id="@+id/contact"
        android:layout_width="130dp"
        android:layout_height="111dp"
        android:background="#00000000"
        android:padding="0dp"
        android:src="@drawable/tab_phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton2"
        tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck" />


    <TextView
        android:id="@+id/imageButton2"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:background="#4ea1d3"
        android:text=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="管理者用メニュー"
        android:textColor="#FFFFFF"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,MissingConstraints" />

    <ImageButton
        android:id="@+id/back"
        android:layout_width="93dp"
        android:layout_height="50dp"
        android:background="#00000000"
        android:padding="0dp"
        android:src="@drawable/tab_back"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="ContentDescription,TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:background="#4ea1d3"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/btnVoice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="222dp"
        android:background="#00000000"
        android:elevation="3dp"
        android:padding="0dp"
        android:src="@drawable/tab_navi_button2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/btnMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="222dp"
        android:background="#00000000"
        android:elevation="3dp"
        android:padding="0dp"
        android:src="@drawable/tab_navi_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>
